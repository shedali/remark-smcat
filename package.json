	"description": "state machine cat remark plugin",
	"license": "MIT",
	"repository": "shedali/remark-smcat",
	"main": "dist/index.js",
	"author": {
		"name": "Franz",
		"email": "shedali@gmail.com",
		"url": "http://github.com/shedali/remark-smcat"
	},
	"engines": {
		"node": ">=12"
	},
	"ava": {
